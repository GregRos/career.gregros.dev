    "esbuild": "^0.19.11",
    "front-matter": "^4.0.2",
    "globby": "^14.0.0",
    "gray-matter": "^4.0.3",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "js-yaml": "^4.1.0",
    "lazies": "^0.6.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nodemon": "^3.0.2",
